{
  "name": "mat-keyboard",
  "version": "0.1.1",
  "description": "Onscreen virtual keyboard for Angular based on material components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-extensions/mat-keyboard.git"
  },
  "author": "David Enke <post@davidenke.de>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Jos√© Toledo Navarro",
      "email": "jotatoledon@gmail.com",
      "url": "https://github.com/jotatoledo"
    }
  ],
  "keywords": [
    "angular",
    "material",
    "keyboard",
    "virtual"
  ],
  "bugs": {
    "url": "https://github.com/ngx-extensions/mat-keyboard/issues"
  },
  "homepage": "https://github.com/ngx-extensions/mat-keyboard",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:demo",
    "start:demo": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "npm run build:core && npm run build:demo && npm run build:docs",
    "build:core": "ng-packagr -p ./src/core/package.json && cpx -p README.md dist/core",
    "build:demo": "ng build --prod --locale=de --progress=false --base-href=/demo/",
    "build:docs": "compodoc ./src/core -p ./tsconfig.json --silent --disableCoverage --disablePrivateOrInternalSupport --theme readthedocs --output ./dist/docs",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:ts": "ng lint --type-check=true",
    "lint:sass": "sass-lint --verbose --no-exit --config ./.sass-lint.yml"
  },
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^7.2.11",
    "@angular/forms": "^7.2.11",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@angular/router": "^7.2.11",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.3",
    "hammerjs": "2.0.8",
    "reset-css": "2.2.1",
    "rxjs": "^6.4.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "^7.3.7",
    "@angular/compiler-cli": "^7.2.11",
    "@angular/language-service": "^7.2.11",
    "@compodoc/compodoc": "1.1.5",
    "@types/jasmine": "2.8.6",
    "@types/node": "9.4.6",
    "codelyzer": "^5.0.0",
    "cpx": "1.5.0",
    "ng-packagr": "^4.0.0",
    "sass-lint": "1.12.1",
    "ts-node": "5.0.0",
    "tsickle": "~0.29.0",
    "tslib": "^1.9.0",
    "tslint": "^5.15.0",
    "typescript": "~3.2.4"
  }
}
