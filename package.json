{
  "name": "mat-keyboard",
  "version": "0.0.0",
  "description": "Onscreen virtual keyboard for Angular based on material components.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-extensions/mat-keyboard.git"
  },
  "author": "David Enke <post@davidenke.de>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Jos√© Toledo Navarro",
      "email": "jotatoledon@gmail.com",
      "url": "https://github.com/jotatoledo"
    }
  ],
  "keywords": [
    "angular",
    "material",
    "keyboard",
    "virtual"
  ],
  "bugs": {
    "url": "https://github.com/ngx-extensions/mat-keyboard/issues"
  },
  "homepage": "https://github.com/ngx-extensions/mat-keyboard",
  "scripts": {
    "start": "ng serve",
    "build": "run-p build:lib build:demo build:docs",
    "build:lib": "rm -rf ./dist/mat-keyboard && ng build mat-keyboard",
    "build:demo": "ng build --prod --base-href=/demo/",
    "build:docs": "compodoc ./projects/mat-keyboard -p ./projects/mat-keyboard/tsconfig.lib.json --silent --disableCoverage --disablePrivateOrInternalSupport --theme readthedocs --output ./dist/docs",
    "test": "ng test mat-keyboard --code-coverage --watch=false --source-map=false",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "lint:md": "markdownlint -i CHANGELOG.md {src,projects}/**/*.md",
    "lint:style": "stylelint {src,projects}/**/*.{scss,css}",
    "lint:style--fix": "npm run lint:style -- --fix",
    "lint:ts": "ng lint",
    "lint:ts--fix": "npm run lint:ts -- --fix",
    "lint": "run-p lint:ts lint:style lint:md",
    "lint--fix": "run-p lint:ts--fix lint:style--fix",
    "format": "prettier -c {src,projects,scripts}/**/*.{ts,js,json,css,scss,md,sh}",
    "format--fix": "npm run format -- --write",
    "changelog--bump": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "changelog--next": "conventional-changelog -i CHANGELOG.md -s -u -p angular"
  },
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "^7.2.11",
    "@angular/compiler": "^7.2.11",
    "@angular/core": "^7.2.11",
    "@angular/forms": "^7.2.11",
    "@angular/material": "^7.3.6",
    "@angular/platform-browser": "^7.2.11",
    "@angular/platform-browser-dynamic": "^7.2.11",
    "@angular/router": "^7.2.11",
    "classlist.js": "1.1.20150312",
    "core-js": "^2.5.3",
    "hammerjs": "2.0.8",
    "reset-css": "^4.0.0",
    "rxjs": "^6.4.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.13.8",
    "@angular-extensions/testing-library": "^3.2.0",
    "@angular/cli": "^7.3.7",
    "@angular/compiler-cli": "^7.2.11",
    "@angular/language-service": "^7.2.11",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~3.3.12",
    "@types/node": "9.4.6",
    "codelyzer": "^5.0.0",
    "conventional-changelog-cli": "^2.0.12",
    "coveralls": "^3.0.3",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-marbles": "~0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "~1.4.0",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.15.0",
    "ng-packagr": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "ts-node": "5.0.0",
    "tsickle": "~0.29.0",
    "tslib": "^1.9.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.4"
  }
}
